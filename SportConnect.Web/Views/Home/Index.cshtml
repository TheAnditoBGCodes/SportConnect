@using SportConnect.Utility;

<style>
    main.pb-3 {
        padding-bottom: 0 !important;
        min-height: 81.5vh;
        display: flex;
        align-items: center;
    }

    .container main {
        width: 100%;
        justify-content: center;
    }

    .dashboard-container {
        min-height: 75vh;
        align-content: center;
        width: 100%;
    }

    .dashboard-title {
        font-weight: bolder;
        color: black;
    }

    .dashboard-card {
        padding: 0.5rem;
        transition: all 0.3s ease;
        border-radius: 12px;
        background-color: #8ee697;
        height: 100%;
        width: 100%;
        display: flex;
        flex-direction: column;
        align-content: space-around;
        align-items: center;
    }

        .dashboard-card:hover {
            transform: translateY(-5px);
        }

    .dashboard-icon {
        margin-bottom: 1rem;
        transition: all 0.3s ease;
    }

    .dashboard-card:hover .dashboard-icon {
        transform: scale(1.05);
    }

    .dashboard-btn {
        min-width: 160px !important;
        width: 100% !important;
        font-weight: 500;
        padding: 0.5rem 1rem;
        border-radius: 8px;
        transition: all 0.3s ease;
    }

        .dashboard-btn:hover {
            transform: translateY(-2px);
        }

    .btn-primary {
        background-color: blue;
        border-color: blue;
    }

    .btn-success {
        background-color: green;
        border-color: green;
    }

    .btn-warning {
        background-color: #AA7700;
        border-color: #AA7700;
    }

    .btn-info {
        background-color: #0aabf5;
        border-color: #0aabf5;
    }


    @@media (max-width: 768px) {
        .dashboard-container {
            padding: 0 10px;
        }

        .dashboard-title {
            text-align: center;
            margin-bottom: 1.5rem;
        }
    }


    @@media (max-width: 480px) {
        .dashboard-card {
            padding: 0.5rem;
            border-radius: 6px;
        }

        .dashboard-title {
            font-size: 1.5rem !important;
            margin-bottom: 0.5rem;
        }

        .dashboard-icon {
            width: 120px !important;
            height: 120px !important;
        }

        .dashboard-btn {
            width: 40% !important;
            font-size: 0.9rem !important;
            min-width: auto !important;
        }
    }

    @@media (max-width: 320px) {
        .dashboard-card {
            padding: 0.25rem;
        }

        .dashboard-title {
            font-size: 1.25rem !important;
        }

        .dashboard-btn {
            width: 50% !important;
            padding: 0.25rem 0.5rem;
            font-size: 0.8rem !important;
        }

        .dashboard-icon {
            width: 100px !important;
            height: 100px !important;
            margin-bottom: 0.5rem;
        }
    }


    @@media (max-height: 600px) {
        .dashboard-container {
            min-height: auto !important;
            padding-top: 1rem;
            padding-bottom: 1rem;
        }

        main.pb-3 {
            min-height: auto;
        }

        .row.mb-4 {
            margin-bottom: 0.25rem !important;
        }
    }


    @@media (min-width: 768px) and (max-width: 1024px) and (orientation: landscape) {
        main.pb-3 {
            min-height: 85vh !important;
        }

        .dashboard-container {
            width: 90% !important;
        }

        .dashboard-icon {
            width: 180px !important;
            height: 180px !important;
        }
    }


    @@media (min-width: 1400px) {
        .dashboard-container {
            max-width: 1320px;
        }

        .dashboard-icon {
            width: 320px !important;
            height: 320px !important;
        }

        .dashboard-title {
            font-size: 2.2rem;
        }

        .dashboard-btn {
            min-width: 200px !important;
            font-size: 1.1rem;
        }
    }


    @@media print {
        main.pb-3 {
            min-height: auto !important;
        }

        .dashboard-btn {
            display: none !important;
        }

        .dashboard-card {
            box-shadow: none !important;
            background-color: transparent !important;
            border: 1px solid #000 !important;
        }

        .dashboard-icon {
            print-color-adjust: exact;
            -webkit-print-color-adjust: exact;
        }
    }


    @@media (max-width: 767px) {
        .col-lg-4,
        .col-md-6 {
            width: 100% !important;
            max-width: 100% !important;
            margin-bottom: 1rem;
        }

        .dashboard-card {
            flex-direction: row;
            justify-content: space-between;
            align-items: center;
            padding: 0.75rem;
        }

        .dashboard-icon {
            margin-bottom: 0;
            margin-right: 1rem;
        }

        .dashboard-btn {
            width: 100% !important;
        }
    }
</style>

